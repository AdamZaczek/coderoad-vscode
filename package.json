{
	"name": "coderoad-vscode",
	"displayName": "coderoad-vscode",
	"description": "Interactive tutorials in your editor",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "Shawn McKay <shawn.j.mckay@gmail.com>",
	"activationEvents": [
		"onCommand:coderoad.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "coderoad.start",
				"title": "Start",
				"category": "CodeRoad"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"machine": "node ./out/state/index.js",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.0.4",
		"prettier": "^1.17.1",
		"tslint": "^5.17.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.5.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"xstate": "^4.6.0"
	}
}
